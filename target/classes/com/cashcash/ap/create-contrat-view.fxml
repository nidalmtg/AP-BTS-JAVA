<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml">
    <padding>
        <Insets bottom="10.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <HBox>
        <Label text="Sélectionner le type de contrat : "/>
    </HBox>
    <HBox>
        <ComboBox fx:id="typeContrat"/>
    </HBox>
    <HBox>
        <Button fx:id="btnCreateContrat" text="Créer un contrat pour le client"/>
    </HBox>
</VBox>
